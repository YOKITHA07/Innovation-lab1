
let i = 1;
while (i <= 10) {
    console.log(i);
    i++;
}
let input;
do {
    input = prompt("Enter the number 5:");
} while (input != 5);
console.log("Correct input!");
let n = 5;
let sum = 0;
for (let j = 1; j <= n; j++) {
    sum += j;
}
console.log("Sum of first", n, "natural numbers is:", sum);
for (let a = 1; a <= 5; a++) {
    for (let b = 1; b <= 10; b++) {
        console.log(`${a} x ${b} = ${a * b}`);
    }
    console.log("-------------");
}
